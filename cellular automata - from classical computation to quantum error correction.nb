Cell[BoxData[{
 RowBox[{"BeginPackage", "[", "\"\<Daedalus`FabricCalculus`\>\"", "]"}], "\n", 
 RowBox[{
  RowBox[{
   StyleBox[
    RowBox[{"FabricMetricTensor", "::", "usage"}], "MessageName"], "=", 
   "\"\<FabricMetricTensor[lat,bw,integrity] \[RightArrow] Association with \
keys    \\\"Metrics\\\" and \\\"TransactionalDistance\\\".\>\""}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   StyleBox[
    RowBox[{"GeodesicTransactionRouter", "::", "usage"}], "MessageName"], 
   "=", "\"\<GeodesicTransactionRouter[topology, weights] \[RightArrow] \
Association with    keys \\\"GraphObject\\\" and \
\\\"CalculateGeodesicPath\\\".\>\""}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Begin", "[", "\"\<`Private`\>\"", "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"ClearAll", "[", "FabricMetricTensor", "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"FabricMetricTensor", "[", 
    RowBox[{
     RowBox[{"lat_", "?", "NumericQ"}], ",", 
     RowBox[{"bw_", "?", "NumericQ"}], ",", 
     RowBox[{"integ_", "?", "NumericQ"}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"m", "=", 
       RowBox[{"{", 
        RowBox[{"lat", ",", 
         FractionBox["1", 
          RowBox[{"Max", "[", 
           RowBox[{"bw", ",", "$MachineEpsilon"}], "]"}]], ",", 
         RowBox[{"1", "-", "integ"}]}], "}"}]}], "}"}], ",", 
     RowBox[{"Association", "[", 
      RowBox[{
       RowBox[{"\"\<Metrics\>\"", "\[Rule]", "m"}], ",", 
       RowBox[{"\"\<TransactionalDistance\>\"", "\[Rule]", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"m", ".", "#1"}], "&"}], ")"}]}]}], "]"}]}], "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{"ClearAll", "[", "GeodesicTransactionRouter", "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"GeodesicTransactionRouter", "[", 
    RowBox[{"topo_Association", ",", 
     RowBox[{"weights", ":", 
      RowBox[{"{", 
       RowBox[{"__", "?", "NumericQ"}], "}"}]}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"edges", ",", "verts", ",", "wts", ",", "g"}], "}"}], ",", 
     RowBox[{
      RowBox[{"edges", "=", 
       RowBox[{"Keys", "[", "topo", "]"}]}], ";", 
      RowBox[{"verts", "=", 
       RowBox[{"DeleteDuplicates", "[", 
        RowBox[{"Flatten", "[", 
         RowBox[{"List", "@@@", "edges"}], "]"}], "]"}]}], ";", 
      RowBox[{"wts", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"topo", "[", "#1", "]"}], "[", 
            "\"\<TransactionalDistance\>\"", "]"}], "[", "weights", "]"}], 
          "&"}], ")"}], "/@", "edges"}]}], ";", 
      RowBox[{"g", "=", 
       RowBox[{"Graph", "[", 
        RowBox[{"verts", ",", "edges", ",", 
         RowBox[{"EdgeWeight", "\[Rule]", "wts"}], ",", 
         RowBox[{
         "GraphLayout", "\[Rule]", "\"\<SpringElectricalEmbedding\>\""}]}], 
        "]"}]}], ";", 
      RowBox[{"Association", "[", 
       RowBox[{
        RowBox[{"\"\<Topology\>\"", "\[Rule]", "topo"}], ",", 
        RowBox[{"\"\<PolicyWeights\>\"", "\[Rule]", "weights"}], ",", 
        RowBox[{"\"\<GraphObject\>\"", "\[Rule]", "g"}], ",", 
        RowBox[{"\"\<CalculateGeodesicPath\>\"", "\[RuleDelayed]", 
         RowBox[{"Function", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"src", ",", "dst"}], "}"}], ",", 
           RowBox[{"Module", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"p", ",", "dist"}], "}"}], ",", 
             RowBox[{
              RowBox[{"p", "=", 
               RowBox[{"FindShortestPath", "[", 
                RowBox[{"g", ",", "src", ",", "dst"}], "]"}]}], ";", 
              RowBox[{"dist", "=", 
               RowBox[{"Total", "[", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{
                    RowBox[{
                    RowBox[{"topo", "[", "#1", "]"}], "[", 
                    "\"\<TransactionalDistance\>\"", "]"}], "[", "weights", 
                    "]"}], "&"}], ")"}], "/@", 
                 RowBox[{"UndirectedEdge", "@@@", 
                  RowBox[{"Partition", "[", 
                   RowBox[{"p", ",", "2", ",", "1"}], "]"}]}]}], "]"}]}], ";", 
              RowBox[{"{", 
               RowBox[{"p", ",", "dist"}], "}"}]}]}], "]"}]}], "]"}]}]}], 
       "]"}]}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"n2nLattice", "=", 

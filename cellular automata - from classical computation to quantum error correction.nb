    RowBox[{"ClearAll", "[", "\"\<Global`*\>\"", "]"}], ";"}], ")"}], " ", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"coords", "=", 
     RowBox[{"{", 
      RowBox[{"t", ",", "x", ",", "y", ",", "z"}], "}"}]}], ";"}], ")"}], " ", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"\[Eta]cov", "=", 
     RowBox[{"DiagonalMatrix", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"-", "1"}], ",", "1", ",", "1", ",", "1"}], "}"}], "]"}]}], 
    ";"}], ")"}], " ", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"\[Eta]con", "=", "\[Eta]cov"}], ";"}], ")"}], " ", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{
     RowBox[{"\[Eta]", "[", 
      RowBox[{"\[Mu]_", ",", "\[Nu]_"}], "]"}], ":=", 
     RowBox[{"\[Eta]cov", "\[LeftDoubleBracket]", 
      RowBox[{"\[Mu]", ",", "\[Nu]"}], "\[RightDoubleBracket]"}]}], ";"}], 
   ")"}], " ", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{
     RowBox[{"\[Eta]Inv", "[", 
      RowBox[{"\[Mu]_", ",", "\[Nu]_"}], "]"}], ":=", 
     RowBox[{"\[Eta]con", "\[LeftDoubleBracket]", 
      RowBox[{"\[Mu]", ",", "\[Nu]"}], "\[RightDoubleBracket]"}]}], ";"}], 
   ")"}], " ", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"A\[Mu]", "=", 
     RowBox[{"{", 
      RowBox[{"0", ",", "0", ",", "0", ",", 
       RowBox[{
        RowBox[{"-", "E0"}], " ", "t"}]}], "}"}]}], ";"}], ")"}], " ", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{
     RowBox[{"Fcov", "[", 
      RowBox[{"\[Mu]_", ",", "\[Nu]_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[PartialD]", 
        RowBox[{
        "coords", "\[LeftDoubleBracket]", "\[Mu]", "\[RightDoubleBracket]"}]], 
       RowBox[{
       "A\[Mu]", "\[LeftDoubleBracket]", "\[Nu]", "\[RightDoubleBracket]"}]}],
       "-", 
      RowBox[{
       SubscriptBox["\[PartialD]", 
        RowBox[{
        "coords", "\[LeftDoubleBracket]", "\[Nu]", "\[RightDoubleBracket]"}]], 
       RowBox[{
       "A\[Mu]", "\[LeftDoubleBracket]", "\[Mu]", 
        "\[RightDoubleBracket]"}]}]}]}], ";"}], ")"}], " ", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"FcovMat", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"Simplify", "[", 
        RowBox[{"Fcov", "[", 
         RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"\[Mu]", ",", "4"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"\[Nu]", ",", "4"}], "}"}]}], "]"}]}], ";"}], ")"}], " ", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{
     RowBox[{"Fcon", "[", 
      RowBox[{"\[Mu]_", ",", "\[Nu]_"}], "]"}], ":=", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", "\[Alpha]", "4"], 
      RowBox[{
       UnderoverscriptBox["\[Sum]", "\[Beta]", "4"], 
       RowBox[{
        RowBox[{"\[Eta]Inv", "[", 

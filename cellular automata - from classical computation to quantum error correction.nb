  RowBox[{"(", 
   RowBox[{
    RowBox[{"FinvF", "=", 
     RowBox[{
      FractionBox["1", "2"], " ", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", "\[Alpha]", "4"], 
       RowBox[{
        UnderoverscriptBox["\[Sum]", "\[Beta]", "4"], 
        RowBox[{
         RowBox[{"Fcon", "[", 
          RowBox[{"\[Alpha]", ",", "\[Beta]"}], "]"}], " ", 
         RowBox[{"Fcov", "[", 
          RowBox[{"\[Alpha]", ",", "\[Beta]"}], "]"}]}]}]}]}]}], ";"}], ")"}],
   " ", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{
     RowBox[{"T", "[", 
      RowBox[{"\[Mu]_", ",", "\[Nu]_"}], "]"}], ":=", 

         RowBox[{"Fcon", "[", 
          RowBox[{"\[Mu]", ",", "\[Alpha]"}], "]"}], " ", 
         RowBox[{"Fcon", "[", 
          RowBox[{"\[Nu]", ",", "\[Alpha]"}], "]"}], " ", 
         RowBox[{"\[Eta]Inv", "[", 
          RowBox[{"\[Alpha]", ",", "\[Alpha]"}], "]"}]}]}], "-", 
       RowBox[{
        FractionBox["1", "4"], " ", 
        RowBox[{"\[Eta]Inv", "[", 
         RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}], " ", "FinvF"}]}], 
      "\[Mu]0"]}], ";"}], ")"}], " ", 
  RowBox[{"(", 
